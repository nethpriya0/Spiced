{
  "version": 2,
  "builds": [
    {
      "src": "apps/nextjs/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/nextjs/$1"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "cd apps/nextjs && npm run build",
  "devCommand": "cd apps/nextjs && npm run dev",
  "outputDirectory": "apps/nextjs/.next"
}